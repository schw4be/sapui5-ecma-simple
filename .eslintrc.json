{
  "env": {
    "browser": true,
    "es6": true,    
    "node": true
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "requireConfigFile": false
  },
  "globals": {
    "sap": true,
    "jQuery": true
  },
  "plugins": [
    "@sap/ui5-jsdocs"
  ],
  "extends": [
    "plugin:@sap/ui5-jsdocs/recommended",
    "eslint:recommended"
  ]
}
